{
  "name": "helloworld",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "server": "nodemon -w server server/server.js -e js",
    "dev-server": " concurrently \"vite\" \"nodemon -w ./Server/server.js ./Server/server.js \"",
    "host": " concurrently \"vite --port 5173 --host\" \"nodemon -w ./Server/server.js ./Server/server.js \"",
    "tsc": "tsc -w Server/modules/auth.ts Server/modules/database.ts",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.22.2",
    "bun": "^0.0.12",
    "chalk": "^5.0.1",
    "concurrently": "^7.3.0",
    "google-auth-library": "^8.5.1",
    "vue": "^3.2.37",
    "vue-router": "^4.1.2",
    "vue3-google-login": "^2.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^16.11.45",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "typescript": "~4.7.4",
    "vite": "^3.0.1",
    "vue-tsc": "^0.38.8"
  }
}
